&usbotg {
	vbus-supply = <&reg_usb_otg_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg_1>;
	disable-over-current;
	status = "okay";
};

&usbh1 {
	status = "okay";
};

&weim {
    status = "disabled";
};

// WIFI
&usdhc1 { 
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc1_4900>;
	vmmc-supply = <&reg_1p8v>;
	bus-width = <4>;
	fsl,cd-controller;
	fsl,wp-controller;
	non-removable;
};


// SD
&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2_4900>;
	vmmc-supply = <&reg_3p3v>;
	bus-width = <4>;
	status = "okay";
	//fsl,cd-controller;
	fsl,wp-controller;	
};

// eMMC
&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3_2>;
	vmmc-supply = <&reg_3p3v>;
	bus-width = <4>;
	status = "okay";
	fsl,cd-controller;
	fsl,wp-controller;
	non-removable;
}; 

