&i2c1 {
	status = "okay";
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1_1>;

	rtc: isl12022@6f {
		compatible = "isl,isl12022";
		reg = <0x6f>;
	};
};

&i2c2 {
	status = "okay";
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2_2>;
};


&flexcan1 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_flexcan1_1>;
        status = "okay";
};

&flexcan2 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_flexcan2_1>;
        status = "okay";
};


&fec {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_enet_4900>;
		phy-mode = "rgmii";
		phy-autoneg = "enable";
		status = "okay";	
};


